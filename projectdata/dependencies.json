[     
    {
        "name": "ohNet",
        "type": "openhome",
        "platform-specific": true,
        "version": "1.39.5615",
        "archive-suffix": "-${debugmode}",
        "main-dir": "${dest}${name}-${platform}-${debugmode}",
        "configure-args": [
            "--ohnet-lib-dir",
            "${main-dir}/lib",
            "--ohnet-include-dir",
            "${main-dir}/include/ohnet"
        ],
        "source-git": "https://github.com/openhome/ohNet.git"
    },
    {
        "name": "yocto_core4_sdk",
        "type": "internal",
        "platform-specific": true,
        "platform-ignore": {
            "Linux-armhf": false,
            "*": true },
        "ignore":	"${platform-ignore[$platform]}",
        "version": "0.1.47",
        "cross-check": false,
        "archive-filename": "linn-fb-glibc-x86_64-linn-image-core-cortexa9t2hf-neon-linn-imx6dl-toolchain-5.15-kirkstone.sh",
        "archive-path":     "${binary-repo}/${name}/${version}/${archive-filename}"
    },
    {
        "name": "buildroot_core4_staging",
        "type": "internal",
        "platform-specific": true,
        "platform-ignore": {
            "Linux-armhf": false,
            "*": true },
        "ignore":	"${platform-ignore[$platform]}",
        "version": "0.2.47",
        "archive-filename": "${name}-${platform}-${version}.tar.gz"
    },
    {
        "name": "ohWafHelpers",
        "type": "openhome",
        "platform-specific": false,
