[     
    {
        "name": "ohNet",
        "type": "openhome",
        "platform-specific": true,
        "platform-version": {
            "*": "1.36.5141",
            "Linux-armhf": "1.36.22-yocto"
        },
        "platform-suffix": {
            "*": "",
            "Linux-armhf": "-yocto"
        },
        "version": "${platform-version[$platform]}", 
        "archive-directory": "${binary-repo}/ohNet${platform-suffix[$platform]}/",
        "archive-suffix": "-${debugmode}",
        "main-dir": "${dest}${name}-${platform}-${debugmode}"
    },
    {   "name": "yocto_core4_sdk",
        "type": "internal",
        "platform-specific": true,
        "platform-ignore": {
            "Linux-armhf": false,
            "*": true },
        "ignore":	"${platform-ignore[$platform]}",
        "version": "0.0.25",
        "cross-check": false,
        "archive-filename": "linn-fb-glibc-x86_64-linn-image-core-cortexa9t2hf-neon-linn-imx6dl-toolchain-5.15-kirkstone-${version}.sh"
    },
    {
        "name": "ohWafHelpers",
        "type": "openhome",
        "platform-specific": false,
        "version": "0.2.141",
        "cross-check": false,
        "archive-filename": "${name}-${version}.tar.gz",
        "source-git": "https://github.com/openhome/ohWafHelpers.git"
    },
    {
        "name": "TestTones",
        "type": "external",
        "version": "1.9",
        "cross-check": false,
        "archive-path": "s3://linn-artifacts-private/testtones/TestTones-${version}.zip",
        "dest": "dependencies/AnyPlatform/TestTones/"
    },
    {
        "name": "libplatform",
        "type": "internal",
        "version": "0.88.1276",
        "cross-check": false,
        "archive-suffix": "-${debugmode}",
        "platform-ignore": {
            "Core-ppc32": false,
            "*": true
        },
        "ignore": "${platform-ignore[$platform]}"
    },
    {
        "name": "libosa",
        "type": "internal",
        "version": "0.2.285",
        "cross-check": false,
        "archive-suffix": "-Release",
        "platform-ignore": {
            "Core-ppc32": false,
            "*": true
        },
        "ignore": "${platform-ignore[$platform]}"
    },
    {
        "name": "libressl",
        "type": "internal",
        "platform-specific": true,
        "version": "0.1.37",
        "archive-filename": "${name}-${version}-${archive-platform}-${debugmode}.tar.gz"
    },
    {
        "name": "raat",
        "type": "openhome",
        "version": "0.0.4",
        "archive-suffix": "-${debugmode}",
        "archive-directory": "${binary-repo}/raat-yocto/",
        "cross-check": false,
        "platform-ignore": {
            "Windows-x86": false,
            "Linux-x64": false,
            "Linux-armhf": false,
            "*": true
        },
        "ignore": "${platform-ignore[$platform]}"
    },
    {
        "name": "testharness",
        "version": "0.1.66",
        "cross-check": false,
        "type": "openhome",
        "archive-filename": "${name}-${version}.tar.gz",
        "dest": "dependencies/AnyPlatform/"
    },
    {
        "name": "Reprog",
        "version": "0.1.70",
        "cross-check": false,
        "type": "internal",
        "archive-filename": "${name}-${version}-${host-platform}-Release.tar.gz",
        "platform-ignore": {
            "Core-ppc32": false,
            "*": true
        },
        "ignore": "${platform-ignore[$platform]}",
        "dest": "dependencies/${host-platform}/Reprog/"
    },
    {
        "name": "hardware",
        "version": "4.855.36595",
        "cross-check": false,
        "type": "internal",
        "archive-filename": "${name}-Core1-${version}.tar.gz",
        "dest": "dependencies/AnyPlatform/${name}/"
    },
    {
        "name": "transmute",
        "version": "0.2.91",
        "cross-check": false,
        "type": "internal",
        "archive-filename": "${name}-${version}.tar.gz",
        "platform-ignore": {
            "Core-ppc32": false,
            "*": true
        },
        "ignore": "${platform-ignore[$platform]}",
        "dest": "dependencies/AnyPlatform/"
    },
    {
        "name": "FunctionalTest",
        "type": "openhome",
        "version": "0.0.1263",
        "cross-check": false,
        "platform-specific": false,
        "archive-filename": "FunctionalTest-${version}.zip",
        "dest": "dependencies/AnyPlatform/FunctionalTest"
    }
]
