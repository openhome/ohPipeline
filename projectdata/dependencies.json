    {
        "name": "libressl",
        "type": "internal",
        "platform-specific": true,
        "platform-version": {
            "*": "0.1.37",
            "Windows-x86": "0.1.37",
            "Core-ppc32": "0.1.37"
        },
        "version": "${platform-version[$platform]}",
        "archive-filename": "${name}-${version}-${archive-platform}-${debugmode}.tar.gz"
    },
    {
        "name": "raat",
        "type": "openhome",
        "version": "1.1.64",
        "archive-suffix": "-${debugmode}",
        "archive-directory": "${binary-repo}/raat/",
        "cross-check": false,
        "platform-ignore": {
            "Windows-x86": false,
            "Linux-x64": false,
            "Linux-armhf": false,
            "*": true
        },
        "ignore": "${platform-ignore[$platform]}"
    },
    {
        "name": "testharness",
        "version": "0.1.66",
        "cross-check": false,
        "type": "openhome",
        "archive-filename": "${name}-${version}.tar.gz",
