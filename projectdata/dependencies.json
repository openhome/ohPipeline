[     
    {
        "name": "ohNet",
        "type": "openhome",
        "platform-specific": true,
        "version": "1.38.61-yocto",
        "archive-suffix": "-${debugmode}",
        "main-dir": "${dest}${name}-${platform}-${debugmode}",
        "configure-args": [
            "--ohnet-lib-dir",
            "${main-dir}/lib",
            "--ohnet-include-dir",
            "${main-dir}/include/ohnet"
        ],
        "source-git": "https://github.com/openhome/ohNet.git"
    },
    {
        "name": "yocto_core4_sdk",
        "type": "internal",
        "platform-specific": true,
        "platform-ignore": {
            "armhf-kirkstone-linux": false,
            "*": true
        },
        "ignore": "${platform-ignore[$platform]}",
        "version": "0.1.5",
        "cross-check": false,
        "archive-filename": "linn-wayland-glibc-x86_64-linn-image-core-cortexa9t2hf-neon-linn-imx6dl-toolchain-5.15-kirkstone.sh",
        "archive-path": "${binary-repo}/${name}/${version}/linn-imx6dl/${archive-filename}"
    },
    {
        "name": "yocto_core5_sdk",
        "type": "internal",
        "platform-specific": true,
        "platform-ignore": {
            "aarch64-kirkstone-linux": false,
            "aarch64-scarthgap-linux": false,
            "*": true
        },
        "platform-version": {
            "aarch64-kirkstone-linux": "0.1.5",
            "aarch64-scarthgap-linux": "0.2.4"
        },
        "platform-archivename": {
            "aarch64-kirkstone-linux": "5.15-kirkstone",
            "aarch64-scarthgap-linux": "6.6-scarthgap"
        },
        "ignore": "${platform-ignore[$platform]}",
        "version": "${platform-version[$platform]}",
        "cross-check": false,
        "archive-filename": "linn-wayland-glibc-x86_64-linn-image-core-armv8a-imx8mp-lpddr4-evk-linn-toolchain-${platform-archivename[$platform]}.sh",
        "archive-path": "${binary-repo}/${name}/${version}/imx8mp-lpddr4-evk-linn/${archive-filename}"
    },
    {
        "name": "buildroot_core4_staging",
        "type": "internal",
        "platform-specific": true,
        "platform-ignore": {
            "armhf-buildroot-linux": false,
            "*": true },
        "ignore":	"${platform-ignore[$platform]}",
        "version": "0.2.146",
        "archive-filename": "${name}-Linux-armhf-${version}.tar.gz"
    },
    {
        "name": "ohWafHelpers",
        "type": "openhome",
        "platform-specific": false,
        "version": "0.2.165",
        "cross-check": false,
        "archive-filename": "${name}-${version}.tar.gz",
        "source-git": "https://github.com/openhome/ohWafHelpers.git"
    },
    {
        "name": "TestTones",
        "type": "external",
        "version": "1.12",
        "cross-check": false,
        "archive-path": "s3://linn-artifacts-private/testtones/TestTones-${version}.zip",
        "dest": "dependencies/AnyPlatform/TestTones/"
    },
    {
        "name": "libplatform",
        "type": "internal",
        "version": "0.93.12-yocto",
        "cross-check": false,
        "archive-suffix": "-${debugmode}",
        "platform-ignore": {
            "Core-ppc32": false,
            "*": true
        },
        "archive-path": "${binary-repo}/${name}/${archive-filename}",
        "ignore": "${platform-ignore[$platform]}"
    },
    {
        "name": "libplatform-host",
        "real-name": "libplatform",
        "type": "internal",
        "version": "0.93.12-yocto",
        "cross-check": false,
        "platform-specific": false,
        "archive-filename": "${real-name}-${version}-${host-platform}-${debugmode}.tar.gz",
        "archive-path": "${binary-repo}/${real-name}/${archive-filename}",
        "dest": "dependencies/${host-platform}/${name}"
    },
    {
        "name": "libosa",
        "type": "internal",
        "version": "0.2.285",
        "cross-check": false,
        "archive-suffix": "-Release",
        "platform-ignore": {
            "Core-ppc32": false,
            "*": true
        },
        "ignore": "${platform-ignore[$platform]}"
    },
    {
        "name": "libressl",
        "type": "internal",
        "platform-specific": true,
        "platform-version": {
            "armhf-buildroot-linux": "0.1.19-yocto",
            "armhf-kirkstone-linux": "0.1.19-yocto",
            "aarch64-kirkstone-linux": "0.1.19-yocto",
            "aarch64-scarthgap-linux": "0.1.19-yocto",
            "Windows-x86": "0.1.37",
            "Core-ppc32": "0.1.37",
            "*": "0.1.19-yocto"
        },
        "version": "${platform-version[$platform]}",
        "archive-filename": "${name}-${version}-${archive-platform}-${debugmode}.tar.gz"
    },
    {
        "name": "raat",
        "type": "openhome",
        "version": "1.1.7-yocto",
        "archive-suffix": "-${debugmode}",
        "archive-directory": "${binary-repo}/raat/",
        "cross-check": false,
        "platform-ignore": {
            "Windows-x86": false,
            "Linux-x64": false,
            "armhf-buildroot-linux": false,
            "armhf-kirkstone-linux": false,
            "aarch64-kirkstone-linux": false,
            "aarch64-scarthgap-linux": false,
            "*": true
        },
        "ignore": "${platform-ignore[$platform]}"
    },
    {
        "name": "testharness",
        "version": "0.1.66",
        "cross-check": false,
        "type": "openhome",
        "archive-filename": "${name}-${version}.tar.gz",
        "dest": "dependencies/AnyPlatform/"
    },
    {
        "name": "Reprog",
        "version": "0.1.70",
        "cross-check": false,
        "type": "internal",
        "archive-filename": "${name}-${version}-${host-platform}-Release.tar.gz",
        "platform-ignore": {
            "Core-ppc32": false,
            "*": true
        },
        "ignore": "${platform-ignore[$platform]}",
        "dest": "dependencies/${host-platform}/Reprog/"
    },
    {
        "name": "hardware",
        "version": "4.855.36595",
        "cross-check": false,
        "type": "internal",
        "archive-filename": "${name}-Core1-${version}.tar.gz",
        "dest": "dependencies/AnyPlatform/${name}/"
    },
    {
        "name": "transmute",
        "version": "0.2.91",
        "cross-check": false,
        "type": "internal",
        "archive-filename": "${name}-${version}.tar.gz",
        "platform-ignore": {
            "Core-ppc32": false,
            "*": true
        },
        "ignore": "${platform-ignore[$platform]}",
        "dest": "dependencies/AnyPlatform/"
    },
    {
        "name": "FunctionalTest",
        "type": "openhome",
        "version": "0.0.1263",
        "cross-check": false,
        "platform-specific": false,
        "archive-filename": "FunctionalTest-${version}.zip",
        "dest": "dependencies/AnyPlatform/FunctionalTest"
    }
]
